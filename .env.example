# .env.example

# ------------------------------------------------------------------------
# Supabase
# URL e chiave pubblica (anon key) del tuo progetto Supabase
VITE_SUPABASE_URL=
VITE_SUPABASE_PUBLISHABLE_KEY=

# ------------------------------------------------------------------------
# Microsoft Translator (Azure Cognitive Services)
# Endpoint dell'API Translator Text (non modifica)
VITE_AZURE_TRANSLATOR_ENDPOINT=https://api.cognitive.microsofttranslator.com
# Chiave API per Translator Text
VITE_AZURE_TRANSLATOR_KEY=

# ------------------------------------------------------------------------
# Anilist GraphQL API
# URL per le chiamate GraphQL ad Anilist
VITE_ANILIST_API_URL=https://graphql.anilist.co
